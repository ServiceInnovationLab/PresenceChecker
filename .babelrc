{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [ "transform-class-properties", { "spec": true } ]
  ],
  "env": {
    "test": {
      "presets": [ [ "env" ], "react", "@babel/preset-react" ],
      "plugins": [
        "syntax-dynamic-import",
        [ "transform-class-properties", { "spec": true } ]
      ]
    }
  }
}
